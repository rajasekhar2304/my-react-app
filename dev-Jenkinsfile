pipeline {
    agent any

    stages {
        stage("SCM Checkout") {
            steps {
                git branch: 'main',
                    credentialsId: "github_access",                
                    url: "https://github.com/rajasekhar2304/my-react-app.git"            
            }
        }
        stage("Build") {
            steps {
                sh "npm install"
                sh "npm run clean"
                sh "npm run build"
            }
        }
        // stage('Clean Up') {
        //     steps {
        //         sh "sh /var/lib/jenkins/workspace/Pipeline_with_SCM/scripts/script.sh"
        //     }
        // }
    }
}